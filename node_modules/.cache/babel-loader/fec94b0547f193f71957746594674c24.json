"<template>\n  <div>\n    <h1>アルバム登録</h1>\n    <form @submit.prevent=\"submitCreate\">\n      <label>Name</label><br />\n      <input v-model=\"form.name\" placeholder=\"Enter album name\" /><br />\n      <input type=\"submit\" value=\"Submit\" />\n    </form>\n  </div>\n</template>\n\n<script>\nimport { API } from \"aws-amplify\";\nimport { createAlbum } from \"../../graphql/mutations\";\n\nexport default {\n  name: \"AlbumCreate\",\n  data() {\n    return {\n      form: {\n        name: \"\",\n      },\n    };\n  },\n  methods: {\n    async submitCreate() {\n      await API.graphql({\n        query: createAlbum,\n        variables: { input: this.form },\n      })\n        .then((result) => {\n          console.log(result);\n          this.$router.push({ name: \"AlbumIndex\" });\n        })\n        .catch((error) => {\n          console.log(error);\n        });\n    },\n  },\n};\n</script>"],"sourceRoot":""}]}